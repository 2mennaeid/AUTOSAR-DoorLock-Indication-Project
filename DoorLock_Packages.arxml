<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>MyDataTypes</SHORT-NAME>
			<ELEMENTS>
				<DATA-CONSTR>
					<SHORT-NAME>DataC_unit8</SHORT-NAME>
					<DATA-CONSTR-RULES>
						<DATA-CONSTR-RULE>
							<PHYS-CONSTRS>
								<LOWER-LIMIT>0</LOWER-LIMIT>
								<UPPER-LIMIT>255</UPPER-LIMIT>
							</PHYS-CONSTRS>
							<INTERNAL-CONSTRS>
								<LOWER-LIMIT>0</LOWER-LIMIT>
								<UPPER-LIMIT>255</UPPER-LIMIT>
							</INTERNAL-CONSTRS>
						</DATA-CONSTR-RULE>
					</DATA-CONSTR-RULES>
				</DATA-CONSTR>
				<SW-BASE-TYPE>
					<SHORT-NAME>My_uint8</SHORT-NAME>
					<CATEGORY>FIXED_LENGTH</CATEGORY>
					<BASE-TYPE-SIZE>8</BASE-TYPE-SIZE>
					<BASE-TYPE-ENCODING>NONE</BASE-TYPE-ENCODING>
					<NATIVE-DECLARATION>unsigned char</NATIVE-DECLARATION>
				</SW-BASE-TYPE>
				<IMPLEMENTATION-DATA-TYPE>
					<SHORT-NAME>u8_t</SHORT-NAME>
					<CATEGORY>VALUE</CATEGORY>
					<SW-DATA-DEF-PROPS>
						<SW-DATA-DEF-PROPS-VARIANTS>
							<SW-DATA-DEF-PROPS-CONDITIONAL>
								<BASE-TYPE-REF DEST="SW-BASE-TYPE">/MyDataTypes/My_uint8</BASE-TYPE-REF>
								<DATA-CONSTR-REF DEST="DATA-CONSTR">/MyDataTypes/DataC_unit8</DATA-CONSTR-REF>
							</SW-DATA-DEF-PROPS-CONDITIONAL>
						</SW-DATA-DEF-PROPS-VARIANTS>
					</SW-DATA-DEF-PROPS>
				</IMPLEMENTATION-DATA-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>Interfaces</SHORT-NAME>
			<ELEMENTS>
				<!-- define SRIF for door state -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_DoorState</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>DoorState</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MyDataTypes/u8_t</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- define CSIF for set led state -->
				<CLIENT-SERVER-INTERFACE>
					<SHORT-NAME>CSIF_LedState</SHORT-NAME>
					<OPERATIONS>
						<CLIENT-SERVER-OPERATION>
							<SHORT-NAME>SetLedState</SHORT-NAME>
							<ARGUMENTS>
								<ARGUMENT-DATA-PROTOTYPE>
									<SHORT-NAME>LedState_IN</SHORT-NAME>
									<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/MyDataTypes/u8_t</TYPE-TREF>
									<DIRECTION>IN</DIRECTION>
								</ARGUMENT-DATA-PROTOTYPE>
							</ARGUMENTS>
							<POSSIBLE-ERROR-REFS>
								<POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/Interfaces/CSIF_LedState/Error_Not_OK</POSSIBLE-ERROR-REF>
								<POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/Interfaces/CSIF_LedState/Error_OK</POSSIBLE-ERROR-REF>
							</POSSIBLE-ERROR-REFS>
						</CLIENT-SERVER-OPERATION>
					</OPERATIONS>
					<POSSIBLE-ERRORS>
						<APPLICATION-ERROR>
							<SHORT-NAME>Error_Not_OK</SHORT-NAME>
							<ERROR-CODE>1</ERROR-CODE>
						</APPLICATION-ERROR>
						<APPLICATION-ERROR>
							<SHORT-NAME>Error_OK</SHORT-NAME>
							<ERROR-CODE>0</ERROR-CODE>
						</APPLICATION-ERROR>
					</POSSIBLE-ERRORS>
				</CLIENT-SERVER-INTERFACE>
			</ELEMENTS>
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>SWCs</SHORT-NAME>
			<ELEMENTS>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SWC_DoorAlgo</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_DoorState</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SRI_DoorState</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_CS_LedState</SHORT-NAME>
							<CLIENT-SERVER-ANNOTATIONS>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/CSIF_LedState</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
							</CLIENT-SERVER-ANNOTATIONS>
						</R-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>IB_SWC_DoorAlgo</SHORT-NAME>
							<EVENTS>
								<TIMING-EVENT>
									<SHORT-NAME>TE_12ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC_DoorAlgo/IB_SWC_DoorAlgo/DoorLock_Indication</START-ON-EVENT-REF>
									<PERIOD>0.012</PERIOD>
								</TIMING-EVENT>
							</EVENTS>
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>DoorLock_Indication</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<DATA-READ-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>DoorState_Value</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/SWCs/SWC_DoorAlgo/RP_DoorState</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/SRI_DoorState/DoorState</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-READ-ACCESSS>
									<SERVER-CALL-POINTS>
										<SYNCHRONOUS-SERVER-CALL-POINT>
											<SHORT-NAME>ServerCall</SHORT-NAME>
											<OPERATION-IREF>
												<CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWCs/SWC_DoorAlgo/RP_CS_LedState</CONTEXT-R-PORT-REF>
												<TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/CSIF_LedState/SetLedState</TARGET-REQUIRED-OPERATION-REF>
											</OPERATION-IREF>
										</SYNCHRONOUS-SERVER-CALL-POINT>
									</SERVER-CALL-POINTS>
									<SYMBOL>DoorLock_Indication_Runnable</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>
				
				<ECU-ABSTRACTION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SWC_DoorSensorAbstraction</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_DoorState</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SRI_DoorState</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_LedSwitch</SHORT-NAME>
							<CLIENT-SERVER-ANNOTATIONS>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/CSIF_LedState</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
							</CLIENT-SERVER-ANNOTATIONS>
						</P-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>IB_SWC_DoorSensorAbstraction</SHORT-NAME>
							<EVENTS>
								<TIMING-EVENT>
									<SHORT-NAME>TE_10ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC_DoorSensorAbstraction/IB_SWC_DoorSensorAbstraction/DoorSensorState</START-ON-EVENT-REF>
									<PERIOD>0.01</PERIOD>
								</TIMING-EVENT>
								<OPERATION-INVOKED-EVENT>
									<SHORT-NAME>OPIE</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC_DoorSensorAbstraction/IB_SWC_DoorSensorAbstraction/LedSwitch_R</START-ON-EVENT-REF>
									<OPERATION-IREF>
										<CONTEXT-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SWCs/SWC_DoorSensorAbstraction/PP_LedSwitch</CONTEXT-P-PORT-REF>
										<TARGET-PROVIDED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/CSIF_LedState/SetLedState</TARGET-PROVIDED-OPERATION-REF>
									</OPERATION-IREF>
								</OPERATION-INVOKED-EVENT>
							</EVENTS>						
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>DoorSensorState</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.00</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>DoorState_val</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/SWCs/SWC_DoorSensorAbstraction/PP_DoorState</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/SRI_DoorState/DoorState</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>
									<SYMBOL>GetDoorState_Runnable</SYMBOL>
								</RUNNABLE-ENTITY>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>LedSwitch_R</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<SYMBOL>Led_Switch_Runnable</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
						
						
					</INTERNAL-BEHAVIORS>
				</ECU-ABSTRACTION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>Composition</SHORT-NAME>
			<ELEMENTS>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Compsit_DoorLock_SWC</SHORT-NAME>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>CompsitSWC_DoorSensorAbstraction</SHORT-NAME>
							<TYPE-TREF DEST="ECU-ABSTRACTION-SW-COMPONENT-TYPE">/SWCs/SWC_DoorSensorAbstraction</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>CompsitSWC_DoorAlgo</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/SWCs/SWC_DoorAlgo</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>ASC_DoorState</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composition/Compsit_DoorLock_SWC/CompsitSWC_DoorSensorAbstraction</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SWCs/SWC_DoorSensorAbstraction/PP_DoorState</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composition/Compsit_DoorLock_SWC/CompsitSWC_DoorAlgo</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWCs/SWC_DoorAlgo/RP_DoorState</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>ASC_LedSwitch</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composition/Compsit_DoorLock_SWC/CompsitSWC_DoorSensorAbstraction</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SWCs/SWC_DoorSensorAbstraction/PP_LedSwitch</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composition/Compsit_DoorLock_SWC/CompsitSWC_DoorAlgo</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWCs/SWC_DoorAlgo/RP_CS_LedState</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>




</AUTOSAR>